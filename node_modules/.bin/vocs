#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules/vocs/_lib/cli/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules/vocs/_lib/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules/vocs/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules/vocs/_lib/cli/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules/vocs/_lib/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules/vocs/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/vocs@1.0.0-alpha.61_@types+react@18.3.11_react-dom@18.3.1_react@18.3.1_rollup@4.24.0_typescript@5.6.3/node_modules:/home/samkuhlmann/Documents/ody/farcastle/farcastle-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vocs/_lib/cli/index.js" "$@"
else
  exec node  "$basedir/../vocs/_lib/cli/index.js" "$@"
fi
