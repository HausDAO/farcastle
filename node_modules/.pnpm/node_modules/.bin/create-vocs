#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/create-vocs@1.0.0-alpha.5/node_modules/create-vocs/_lib/node_modules:/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/create-vocs@1.0.0-alpha.5/node_modules/create-vocs/node_modules:/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/create-vocs@1.0.0-alpha.5/node_modules:/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/create-vocs@1.0.0-alpha.5/node_modules/create-vocs/_lib/node_modules:/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/create-vocs@1.0.0-alpha.5/node_modules/create-vocs/node_modules:/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/create-vocs@1.0.0-alpha.5/node_modules:/Users/earth/Sites/crypto/DAO/farcastle/farcastle/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-vocs/_lib/bin.js" "$@"
else
  exec node  "$basedir/../create-vocs/_lib/bin.js" "$@"
fi
